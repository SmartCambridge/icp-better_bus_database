LOAD CSV
    FROM stdin WITH ENCODING utf8
    INTO postgresql:///acp?siri_vm
       TARGET COLUMNS
       (  --acp_id,
          --location4d,
          ---acp_ts,
          info
       )
    WITH drop indexes,
         fields optionally enclosed by '"',
         fields escaped by double-quote,
         fields terminated by ','

    SET work_mem to '32 MB', maintenance_work_mem to '64 MB';

